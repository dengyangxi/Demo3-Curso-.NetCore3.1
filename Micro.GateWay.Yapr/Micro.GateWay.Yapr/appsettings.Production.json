{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ReverseProxy": {
        "Routes": {
            "route11": {
                "ClusterId": "cluster11",
                "Match": {
                    "Path": "{**catch-all}"
                }
            }
        },
        "Clusters": {
            "cluster11": {
                "Destinations": {
                    "destination1": {
                        "Address": "https://example.com/"
                    }
                }
            }
        }
    }
}
